{
    "name": "kslimani/laravel-geo",
    "description": "Laravel Geo.",
    "keywords": ["country", "currency", "geo", "laravel", "locale"],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "florianv/laravel-swap": "^1.1",
        "illuminate/config": "~5.4",
        "illuminate/console": "~5.4",
        "illuminate/container": "~5.4",
        "illuminate/contracts": "~5.4",
        "illuminate/support": "~5.4",
        "maxmind-db/reader": "^1.1",
        "moneyphp/money": "^3.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "umpirsky/country-list": "^2.0",
        "umpirsky/currency-list": "^1.0",
        "umpirsky/language-list": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Sk\\Geo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sk\\Geo\\GeoServiceProvider"
            ],
            "aliases": {
                "Geo": "Sk\\Geo\\Facades\\Geo"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
